#include <iostream>
#include <cmath>


int main() {
	int t = 1;
	int n = 0;
	int c1 = 0;
	int c2 = 0;
	int c3 = 0;
	int c4 = 0;
	char c;

	std::cin >> t;

	for (int i = 0; i < t; i++) {
		std::cin >> n;
		c1 = 0;
		c2 = 0;
		c3 = 0;
		c4 = 0;

		for (int j = 0; j < n * n; j++) {
			std::cin >> c;

			if (c == '*') {
				if (c1 == 0) {
					c1 = j;
				}
				else {
					c2 = j;
				}
			}
		}

		if (c1 / n == c2 / n) {
			if (c1 / n > 0) {
				c3 = c1 % n;
				c4 = c2 % n;
			}
			else {
				c3 = n + c1 % n;
				c4 = n + c2 % n;
			}
		}
		else {
			if (c1 % n == c2 % n) {
				if (c1 % n > 0) {
					c3 = c1 / n * n;
					c4 = c2 / n * n;
				}
				else {
					c3 = 1 + c1 / n * n;
					c4 = 1 + c2 / n * n;
				}
			}
			else {
				c3 = c1 / n * n + (c2 % n);
				c4 = c2 / n * n + (c1 % n);
			}
		}

		int ct = 0;
		for (int j = 0; j < n; j++) {
			for (int k = 0; k < n; k++) {
				if ((c1 == ct) || (c2 == ct) || (c3 == ct) || (c4 == ct)) {
					std::cout << '*';
				}
				else {
					std::cout << '.';
				}
				ct++;
			}
			std::cout << "\n";
		}
	}
	return 0;
}
