#include <iostream>
#include <cmath>


int main()
{
	int t = 0;
	int n = 0;
	int c1 = 0, c2 = 0;
	int x1 = 0, x2 = 0, y1 = 0, y2 = 0, x3 = 0, x4 = 0, y3 = 0, y4 = 0;
	char c;

	std::cin >> t;


	for (int i = 0; i < t; i++)
	{
		std::cin >> n;

		for (int j = 0; j < n * n; j++)
		{
			std::cin >> c;

			if (c == '*')
			{
				if (c1 == 0)
				{
					c1 = j;
				}
				else
				{
					c2 = j;
				}
			}
		}

		x1 = c1 % n;
		x2 = c2 % n;
		y1 = c1 / n;
		y2 = c2 / n;

		if (x1 == x2)
		{
			if (x1 < n / 2)
			{
				x3 = x1 + std::abs(y1 - y2);
			}
			else
			{
				x3 = x1 - std::abs(y1 - y2);
			}
			x4 = x3;
			y3 = y1;
			y4 = y2;
		}
		else 
		{
			if (y1 == y2)
			{
				if (y1 < n / 2)
				{
					y3 = y1 + std::abs(x1 - x2);
				}
				else
				{
					y3 = y1 - std::abs(x1 - x2);
				}
				y4 = y3;
				x3 = x1;
				x4 = x2;
			}
			else
			{
				x3 = x2;
				y3 = y1;
				x4 = x1;
				y4 = y2;
			}
		}

		for (int j = 0; j < n * n; j++)
		{

		}


	}


	return 0;
}
