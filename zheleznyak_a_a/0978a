#include <iostream>
#include <vector>
 
int main()
{
    int n = 0, x = 0;
    std::cin >> n;
    std::vector <int> a(n);
    std::vector <int> a1;
    
    for (int i = 0; i < n; i ++)
    {
        std::cin >> a[i];
    }
    
    for (int i = n - 1; i >= 0; i --)
    {
        int n_rep = 1;
        for (int j = 0; j < a1.size(); j ++)
        {
            if (a[i] == a1[j])
            {
                n_rep = 0;
            }
        }
        
        if (n_rep == 1)
        {
            a1.push_back(a[i]);
            x += 1;
        }
    }
    
    std::cout << x << "\n";
    
    for (int i = a1.size() - 1; i >= 0; i --)
    {
        std::cout << a1[i] << " ";
    }
}
