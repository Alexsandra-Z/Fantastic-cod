#define _USE_MATH_DEFINES
#include <iostream>
#include <string>
#include <cmath>
 
int main()
{
	int t = 1; // количество строк ввода
	std::cin >> t;
	int flag = 0; // проверка на пригодность числа
	int n = 0; // количество цифр в искомом числе
	std::string ch_str = ""; // строчная запись проверяемого числа
	int c = 0; // цифра проверяемого числа
 
 
	for (int i = 0; i < t; i++)
	{
		std::cin >> n;
 
		for (int ch = pow(10, n - 1); ch < pow(10, n); ch ++) // перебор чесел
		{
			ch_str = std::to_string(ch);
			//std::cout << ch << "\n";
			flag = 1;
 
			for (char j : ch_str) // перебоор цифр числа
			{
				c = j - '0';
				//std::cout << c << ' ';
 
				if ((c == 0) || (c == 1) || (c == 5)) // проверки по заданию
				{
					flag = 0;
					break;
				}
				else 
				{
					if (ch % c == 0)
					{
						flag = 0;
						break;
					}
				}
			}
 
			if (flag == 1)
			{
				std::cout << ch << "\n";
				break;
			}
		}
		if (flag == 0)
		{
			std::cout << -1 << "\n";
		}
	}
 
	return 0;
}
