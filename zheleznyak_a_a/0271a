#include <iostream>
 
int main()
{
  int y = 0;
  int a = 0, b = 0, c = 0, d = 0;
  std::cin >> y;
  y += 1;
  a = y / 1000;
  b = y / 100 % 10;
  c = y / 10 % 10;
  d = y % 10;
  
  //bool t = ((a == b) || (a == c) || (a == d) || (b == c) || (b == d) || (c == d));
  //std::cout << t;
  
  while ((a == b) || (a == c) || (a == d) || (b == c) || (b == d) || (c == d)) {
      y += 1;
      a = y / 1000;
      b = y / 100 % 10;
      c = y / 10 % 10;
      d = y % 10;
  }
  std::cout << y;
  
}
